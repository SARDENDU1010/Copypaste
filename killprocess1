${server:regex/^[^:]+:(.*)$/$1/}
/^$app:.*/
/(.*):\d+/\1/
label_values(up{app="$app"}, instance)

100 - (avg by (instance) (rate(windows_cpu_time_total{mode="idle", instance=~"$server"}[5m])) * 100)
100 - (avg by (instance) (irate(windows_cpu_time_total{mode="idle", instance=~"$server"}[1m])) * 100)
